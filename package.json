{
  "name": "suggest-server",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "mocha --require test/setup.js --watch",
    "dev": "nodemon src/server.js",
    "migrate": "postgrator --config postgrator-config.js",
    "migrate:test": "env NODE_ENV=test npm run migrate",
    "migrate:production": "DATABASE_URL=$(heroku config:get DATABASE_URL) npm run migrate",
    "start": "node src/server.js",
    "predeploy": "npm audit && npm run migrate:production",
    "deploy": "git push heroku master"
  },
  "keywords": [],
  "engines": {
    "node": "16.15.1"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "helmet": "^5.1.0",
    "heroku": "^7.60.2",
    "knex": "^2.4.0",
    "moment": "^2.29.3",
    "morgan": "^1.10.0",
    "pg": "^8.7.3",
    "postgrator": "^7.1.0",
    "postgres": "^3.2.4",
    "sqlite3": "^5.0.8",
    "strip-eof": "^2.0.0",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "chai": "^4.3.6",
    "mocha": "^10.0.0",
    "nodemon": "^2.0.16",
